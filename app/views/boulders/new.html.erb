<%= form_for @boulder, remote: true, authenticity_token: true do |f| %>
	<%= f.label :picture %>
	<%= f.file_field :picture, id: "fileToUpload", onchange: "fileSelected();", accept: "image/*", capture: "camera" %><br>
	<%= f.submit "Save", id: "btn_submitPic" %>
<% end %>
  
<div id="details"></div>
<div id="progress"></div>